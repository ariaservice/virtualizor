{
    "name": "ariaservice/virtualizor",
    "description": "A Laravel 11 package for virtualization",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sajad Mazaheri",
            "email": "me@sajadmazaheri.ir"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ariaservice\\Virtualizor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ariaservice\\Virtualizor\\VirtualizorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
